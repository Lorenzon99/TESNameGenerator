<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_generator">

    <fragment
        android:id="@+id/navigation_generator"
        android:name="com.example.tesnamegenerator.ui.generator.GeneratorFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_generator" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.tesnamegenerator.ui.list.ListFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_saved"
        android:name="com.example.tesnamegenerator.ui.saved.SavedFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_saved" />
</navigation>